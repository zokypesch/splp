# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_GROUP_ID=dukcapil-service
KAFKA_CLIENT_ID=dukcapil-service
KAFKA_CONSUMER_TOPIC=service-1-topic
KAFKA_PRODUCER_TOPIC=command-center-inbox
KAFKA_AUTO_OFFSET_RESET=earliest
KAFKA_ENABLE_AUTO_COMMIT=true
KAFKA_SESSION_TIMEOUT_MS=30000
KAFKA_HEARTBEAT_INTERVAL=3000

# Encryption Configuration (REQUIRED)
ENCRYPTION_KEY=your-32-character-hex-encryption-key-here

# Service Configuration
SERVICE_NAME=Dukcapil Service
SERVICE_VERSION=1.0.0
SERVICE_ENVIRONMENT=development
SERVICE_LOG_LEVEL=info

# Example with production values:
# KAFKA_BROKERS=10.70.1.23:9092,10.70.1.24:9092,10.70.1.25:9092
# SERVICE_ENVIRONMENT=production
# SERVICE_LOG_LEVEL=warn